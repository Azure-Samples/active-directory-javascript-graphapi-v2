{
    "Sample": {
      "Title": "A simple JavaScript single-page application calling Microsoft Graph API using msal.js (w/ AAD v2 endpoint)",
      "Level": 100,
      "Client": "SinglePageApplication"
    },
    "AppRegistrations": [
      {
        "x-ms-id": "JavaScriptSpa",
